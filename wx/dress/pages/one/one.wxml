<view class="lunbo">
		<swiper indicator-dots="{{indicatorDots}}"
  	autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
 	 	<block wx:for="{{imgUrls}}">
    	<swiper-item>
      	<image src="{{item}}" class="slide-image" />
    	</swiper-item>
  	</block>
		</swiper>
	</view>
	<view class='one' wx:for='{{Item}}'>
		<view class='hand'>{{item.pic}}</view>
		<view class='handle'>{{item.desc}}</view>
		<view class='head'>
			<span class='span1'>￥9999</span>
			<span class='span2'>门市价1999</span>
		</view>
	</view>