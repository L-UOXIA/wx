/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
	background: #f2f2f2;
	margin-bottom: 160rpx;
} 
.lunbo{
	width:100%;
}
.slide-image{
	width:100%;
	height: 100%;
}
@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:20px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-shouye:before { content: "\e600"; }

.icon-i:before { content: "\e680"; }

.icon-tel:before { content: "\e613"; }

.icon-webicon302:before { content: "\e695"; }

.icon-gift:before { content: "\e63c"; }

.icon-shijian:before { content: "\e638"; }

.icon-xiaoxi:before { content: "\e601"; }

.icon-xinxing:before { content: "\e659"; }

.icon-qian:before { content: "\e62b"; }


.fixd{
	position:fixed;
	bottom: 0;
	left: 0;
	width: 100%;
	height: 160rpx;
	background: #fff;
	border-top:1rpx solid #999;
	display: flex;
	justify-content: space-around;
	align-items: center;
}
.fixd span{
	display: block;
	text-align: center;
	line-height: 60rpx;
	color: #666;
}
.btn-yes span{
	color: #e20064;
}